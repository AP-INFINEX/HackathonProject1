{
  "manifest_version": 3,
  "name": "Custom Dashboard",
  "version": "1.0",
  "description": "Personalized Chrome Dashboard with background, weather, tasks, and links.",
  
  "icons": {
    "128": "icons/icon.png"
  },

  "action": {
    "default_icon": {
      "128": "icons/icon.png"
    },
    "default_title": "Custom Dashboard"
  },

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": [
    "storage",
    "geolocation"
  ]
}
